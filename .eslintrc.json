{
    "extends": ["airbnb", "prettier"],
    "plugins": ["react", "jsx-a11y", "import"],
    "settings": {
      "import/core-modules": []
    },
      "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn",
        "react/prefer-stateless-function": "off",
        "react/prop-types": "off",
        "react/no-danger": "off"
    }
}